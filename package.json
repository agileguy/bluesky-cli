{
  "name": "bsky-cli",
  "version": "1.0.2",
  "description": "Command-line interface for Bluesky/ATProto social networking",
  "keywords": [
    "bluesky",
    "cli",
    "atproto",
    "social",
    "bsky",
    "at-protocol",
    "social-network",
    "command-line"
  ],
  "author": "agileguy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agileguy/bluesky-cli.git"
  },
  "homepage": "https://github.com/agileguy/bluesky-cli#readme",
  "bugs": {
    "url": "https://github.com/agileguy/bluesky-cli/issues"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "bsky": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "lint": "eslint src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@atproto/api": "^0.18.20",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "ora": "^9.3.0"
  }
}
